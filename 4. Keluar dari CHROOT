# Gunakan Perintah dibawah ini jika keluar dari chroot
apt-get clean && apt-get autoremove && rm -rf /tmp/* ~/.bash_history && umount /proc && umount /sys && umount /dev/pts
exit
sudo umount edit/dev && sudo umount mnt
